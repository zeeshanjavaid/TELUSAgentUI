  "keywords" : [ "DMP", "Application Workbench", "Component" ],
  "dependencies" : [ {
    "id" : "istioflag",
    "kind" : "data",
    "type" : "string",
    "name" : "istioflag",
    "description" : "Variable to control istioInjection",
    "minOccurs" : 1,
    "maxOccurs" : 1,
    "defaultValue" : "true"
  }, {
    "id" : "lcmDesignCpuLimit",
    "kind" : "data",
    "type" : "string",
    "name" : "lcmDesignCpuLimit",
    "description" : "The limiting value for CPU allocation for the component container runtimes in DMP Design LCM",
    "minOccurs" : 1,
    "maxOccurs" : 1,
    "defaultValue" : "2000m"
  }, {
    "id" : "lcmDesignCpuRequest",
    "kind" : "data",
    "type" : "string",
    "name" : "lcmDesignCpuRequest",
