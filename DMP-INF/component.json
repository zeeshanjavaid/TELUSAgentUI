    "minOccurs" : 1,
    "maxOccurs" : 1,
    "defaultValue" : "400Mi"
  }, {
    "id" : "lcmDesignReplicasMax",
    "kind" : "data",
    "type" : "string",
    "name" : "lcmDesignReplicasMax",
