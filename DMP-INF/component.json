    "maxOccurs" : 1,
    "defaultValue" : "1000m"
  }, {
    "id" : "lcmProductionMemoryLimit",
    "kind" : "data",
    "type" : "string",
    "name" : "lcmProductionMemoryLimit",
    "description" : "The limiting value for memory allocation for the component container runtimes in DMP Production LCM",
    "minOccurs" : 1,
    "maxOccurs" : 1,
    "defaultValue" : "4Gi"
  }, {
    "id" : "lcmProductionMemoryRequest",
    "kind" : "data",
    "type" : "string",
    "name" : "lcmProductionMemoryRequest",
    "description" : "The initial memory allocation for the component container runtimes in DMP Production LCM",
    "minOccurs" : 1,
    "maxOccurs" : 1,
    "defaultValue" : "2Gi"
  }, {
    "id" : "lcmProductionReplicasMax",
    "kind" : "data",
    "type" : "string",
    "name" : "lcmProductionReplicasMax",
    "description" : "The maximum permitted replication factor for the component container runtimes in DMP Production LCM",
    "minOccurs" : 1,
    "maxOccurs" : 1,
    "defaultValue" : "6"
  }, {
    "id" : "lcmProductionReplicasMin",
    "kind" : "data",
    "type" : "string",
    "name" : "lcmProductionReplicasMin",
    "description" : "The minimum and initial replication factor for the component container runtimes in DMP Production LCM",
    "minOccurs" : 1,
    "maxOccurs" : 1,
    "defaultValue" : "2"
  }, {
    "kind" : "service",
    "name" : "TELUSAgentUIDB",
    "type" : "MySQL",
    "id" : "TELUSAgentUIDB",
    "env" : [ "design", "staging", "production" ],
    "dbVendor" : "mysql",
    "scope" : "solution",
    "serviceId" : "TELUSAgentUIDB"
  }, {
    "kind" : "engine",
    "type" : "kubernetes",
    "name" : "Tomcat container",
    "env" : [ "design" ],
    "lockConfiguration" : false,
    "size" : "medium",
    "scale" : true,
    "disableAutoScaling" : true,
    "envVars" : {
      "DMP_NOT_SECURED_ENDPOINTS" : "${DMP_NOT_SECURED_ENDPOINTS},OPTIONS:/.*",
      "DMP_CONFIG_DIR" : "/app-root/runtime/config/",
      "ISTIO_INJECTION" : "${istioflag}",
      "LOG4J_FORMAT_MSG_NO_LOOKUPS" : "true",
      "LCM_DESIGN_CPU_LIMIT" : "${lcmDesignCpuLimit}",
      "LCM_DESIGN_CPU_REQUEST" : "${lcmDesignCpuRequest}",
      "LCM_DESIGN_MEM_LIMIT" : "${lcmDesignMemoryLimit}",
      "LCM_DESIGN_MEM_REQUEST" : "${lcmDesignMemoryRequest}",
      "LCM_DESIGN_MAX_REPLICAS" : "${lcmDesignReplicasMax}",
      "LCM_DESIGN_MIN_REPLICAS" : "${lcmDesignReplicasMin}"
    },
    "kubespec" : "DMP-INF/manifest/design/deployment.yaml"
  }, {
    "kind" : "engine",
    "type" : "kubernetes",
    "name" : "Tomcat container",
    "env" : [ "staging" ],
    "lockConfiguration" : false,
    "size" : "medium",
    "scale" : true,
    "disableAutoScaling" : false,
    "envVars" : {
      "DMP_NOT_SECURED_ENDPOINTS" : "${DMP_NOT_SECURED_ENDPOINTS},OPTIONS:/.*",
      "DMP_CONFIG_DIR" : "/app-root/runtime/config/",
      "ISTIO_INJECTION" : "${istioflag}",
      "LOG4J_FORMAT_MSG_NO_LOOKUPS" : "true",
      "LCM_STAGE_CPU_LIMIT" : "${lcmStageCpuLimit}",
      "LCM_STAGE_CPU_REQUEST" : "${lcmStageCpuRequest}",
      "LCM_STAGE_MEM_LIMIT" : "${lcmStageMemoryLimit}",
      "LCM_STAGE_MEM_REQUEST" : "${lcmStageMemoryRequest}",
      "LCM_STAGE_MAX_REPLICAS" : "${lcmStageReplicasMax}",
      "LCM_STAGE_MIN_REPLICAS" : "${lcmStageReplicasMin}"
    },
    "kubespec" : "DMP-INF/manifest/stage/deployment.yaml"
  }, {
    "kind" : "engine",
    "type" : "kubernetes",
    "name" : "Tomcat container",
    "env" : [ "production" ],
    "lockConfiguration" : false,
    "size" : "medium",
    "scale" : true,
    "disableAutoScaling" : false,
    "envVars" : {
      "DMP_NOT_SECURED_ENDPOINTS" : "${DMP_NOT_SECURED_ENDPOINTS},OPTIONS:/.*",
      "DMP_CONFIG_DIR" : "/app-root/runtime/config/",
      "ISTIO_INJECTION" : "${istioflag}",
      "LOG4J_FORMAT_MSG_NO_LOOKUPS" : "true",
      "LCM_PROD_CPU_LIMIT" : "${lcmProductionCpuLimit}",
      "LCM_PROD_CPU_REQUEST" : "${lcmProductionCpuRequest}",
      "LCM_PROD_MEM_LIMIT" : "${lcmProductionMemoryLimit}",
      "LCM_PROD_MEM_REQUEST" : "${lcmProductionMemoryRequest}",
      "LCM_PROD_MAX_REPLICAS" : "${lcmProductionReplicasMax}",
      "LCM_PROD_MIN_REPLICAS" : "${lcmProductionReplicasMin}"
    },
    "kubespec" : "DMP-INF/manifest/production/deployment.yaml"
  } ],
  "license" : "public/license.txt",
  "documentation" : "public/readme.html",
  "authors" : [ {
    "name" : "${project.owner.name}",
