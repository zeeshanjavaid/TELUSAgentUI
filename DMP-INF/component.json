    "minOccurs" : 1,
    "maxOccurs" : 1,
    "defaultValue" : "1"
  }, {
    "id" : "lcmDesignReplicasMin",
    "kind" : "data",
    "type" : "string",
    "name" : "lcmDesignReplicasMin",
    "description" : "The minimum and initial replication factor for the component container runtimes in DMP Design LCM",
    "minOccurs" : 1,
    "maxOccurs" : 1,
    "defaultValue" : "1"
  }, {
    "id" : "lcmStageCpuLimit",
    "kind" : "data",
    "type" : "string",
    "name" : "lcmStageCpuLimit",
    "description" : "The limiting value for CPU allocation for the component container runtimes in DMP Stage LCM",
    "minOccurs" : 1,
    "maxOccurs" : 1,
    "defaultValue" : "2000m"
  }, {
    "id" : "lcmStageCpuRequest",
    "kind" : "data",
    "type" : "string",
    "name" : "lcmStageCpuRequest",
    "description" : "The initial CPU allocation for the component container runtimes in DMP Stage LCM",
    "minOccurs" : 1,
    "maxOccurs" : 1,
    "defaultValue" : "250m"
  }, {
    "id" : "lcmStageMemoryLimit",
    "kind" : "data",
    "type" : "string",
    "name" : "lcmStageMemoryLimit",
    "description" : "The limiting value for memory allocation for the component container runtimes in DMP Stage LCM",
    "minOccurs" : 1,
    "maxOccurs" : 1,
    "defaultValue" : "4Gi"
  }, {
    "id" : "lcmStageMemoryRequest",
    "kind" : "data",
    "type" : "string",
    "name" : "lcmStageMemoryRequest",
